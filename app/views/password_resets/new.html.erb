<%= form_tag password_resets_path, :method => :post do %>

   <div class="col-sm-offset-3 col-sm-6 col-xs-12">
    <h1> Reset password </h1>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
    <%= submit_tag "Reset Password", class: "btn signup_btn2" %>
  </div>
<% end %>